{
  "name": "Instant Howard The Alien",
  "version": "1.0.0",
  "description": "Instant pop out the Howard The Alien meme",
  "permissions" : ["activeTab", "declarativeContent", "storage"],
  "background" : {
    "scripts" : ["background.js"],
    "persistent": false
  },
  "page_action":{
    "default_popup" : "popup.html",
    "default_icon": {
      "16": "images/icon16x16.jpg",
      "32": "images/icon32x32.jpg",
      "48": "images/icon48x48.jpg",
      "128": "images/icon128x128.jpg"
    }
  },
  "icons": {
    "16": "images/icon16x16.jpg",
    "32": "images/icon32x32.jpg",
    "48": "images/icon48x48.jpg",
    "128": "images/icon128x128.jpg"
  },
  "manifest_version": 2
}
